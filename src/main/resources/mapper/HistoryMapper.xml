<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.aisw.mapper.HistoryMapper">
	<select id="getHistory" 
			parameterType="net.aisw.dto.HistoryDTO"
			resultType="net.aisw.dto.HistoryDTO">
		SELECT * FROM history
		WHERE DATE_FORMAT(h_date, '%m-%d') = DATE_FORMAT(CURDATE(), '%m-%d')
	</select>
</mapper>